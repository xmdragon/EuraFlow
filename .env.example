# EuraFlow Environment Configuration Example
# Copy this to .env and configure your settings

# Database
EF__DB_HOST=localhost
EF__DB_PORT=5432
EF__DB_NAME=euraflow
EF__DB_USER=euraflow
EF__DB_PASSWORD=your_password_here

# Redis
EF__REDIS_HOST=localhost
EF__REDIS_PORT=6379
EF__REDIS_DB=0
EF__REDIS_PASSWORD=

# API Settings
EF__API_HOST=0.0.0.0
EF__API_PORT=8000
EF__API_PREFIX=/api/ef/v1
EF__API_TITLE=EuraFlow API
EF__API_DEBUG=true

# Security
EF__SECRET_KEY=your-secret-key-here-change-in-production
EF__ALGORITHM=HS256
EF__ACCESS_TOKEN_EXPIRE_MINUTES=30

# Celery
EF__CELERY_BROKER_URL=redis://localhost:6379/0
EF__CELERY_RESULT_BACKEND=redis://localhost:6379/1

# Monitoring
EF__LOG_LEVEL=INFO
EF__LOG_FORMAT=json
EF__METRICS_ENABLED=true

# Plugin Settings
EF__PLUGIN_DIR=plugins
EF__PLUGIN_AUTO_LOAD=true